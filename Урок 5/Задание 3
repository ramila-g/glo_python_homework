ticket = int(input())

num1 = ticket % 10
num2 = ticket // 10 % 10
num3 = ticket // 100 % 10
num4 = ticket // 1000 % 10
num5 = ticket // 10000 % 10
num6 = ticket // 100000 % 10

if num1 + num2 + num3 == num4 + num5 + num6:
    print('Билет', ticket, 'счастливый')
else:
    print('Билет', ticket, 'НЕсчастливый')
